void teematriisi(float m[3][3], float a, float b, float c) {
  float sa, ca, sb, cb, sc, cc;
  sa=fsin(a); ca=fcos(a);
  sb=fsin(b); cb=fcos(b);
  sc=fsin(c); cc=fcos(c);
  m[0][0]=-sa*sb*cc-ca*sc;
  m[0][1]=-sa*cb;
  m[0][2]= ca*cc-sa*sb*sc;
  m[1][0]= ca*sb*cc-sa*sc;
  m[1][1]= ca*cb;
  m[1][2]= sa*cc+ca*sb*sc;
  m[2][0]= cb*cc;
  m[2][1]=-sb;
  m[2][2]= cb*sc;
}


